<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/app_bg"
    tools:context="ufam.promobile.museudosmisterios.Sandalia.Sandalia_26">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView_sandalia_26_1"
        android:layout_alignParentTop="true"
        android:fontFamily="@string/fonteFamily"
        android:textSize="@dimen/tam_font_large"
        android:textColor="@color/cor_fonte"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView_sandalia_26"
        android:layout_below="@+id/textView_sandalia_26_1"
        android:layout_centerHorizontal="true"
        android:src="@drawable/sandalia_26_panela_menor"
        android:layout_marginTop="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView_sandalia_26_2"
        android:layout_below="@+id/imageView_sandalia_26"
        android:layout_centerHorizontal="true"
        android:fontFamily="@string/fonteFamily"
        android:textSize="20sp"
        android:textColor="@color/cor_fonte"
        android:layout_marginTop="10dp"/>
</RelativeLayout>
