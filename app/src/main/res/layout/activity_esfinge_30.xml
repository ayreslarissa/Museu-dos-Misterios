<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/app_bg"
    tools:context="ufam.promobile.museudosmisterios.Esfinge.Esfinge_30">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_esfinge_30_1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:fontFamily="@string/fonteFamily"
        android:textSize="@dimen/tam_font_normal"
        android:textColor="@color/cor_fonte"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_esfinge_30_2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:fontFamily="@string/fonteFamily"
        android:textStyle="italic"
        android:textSize="20sp"
        android:textColor="@color/cor_fonte"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView_esfinge_30_3"
        android:fontFamily="@string/fonteFamily"
        android:textSize="20sp"
        android:textColor="@color/cor_fonte"
        android:layout_below="@+id/textView_esfinge_30_2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="30dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView_esfinge_30_a"
        android:layout_below="@+id/textView_esfinge_30_3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/esfinge_30_a"
        android:visibility="invisible" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView_esfinge_30_d"
        android:layout_below="@+id/textView_esfinge_30_3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/esfinge_30_d"
        android:visibility="invisible" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView_esfinge_30_c"
        android:src="@drawable/esfinge_30_c"
        android:layout_below="@+id/textView_esfinge_30_3"
        android:layout_toRightOf="@+id/imageView_esfinge_30_b"
        android:layout_toEndOf="@+id/imageView_esfinge_30_b"
        android:layout_marginLeft="70dp"
        android:layout_marginStart="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView_esfinge_30_b"
        android:layout_below="@+id/textView_esfinge_30_3"
        android:layout_toRightOf="@+id/imageView_esfinge_30_a"
        android:layout_toEndOf="@+id/imageView_esfinge_30_a"
        android:layout_marginLeft="57dp"
        android:layout_marginStart="57dp"
        android:src="@drawable/esfinge_30_b"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_proximo"
        android:id="@+id/textView_esfinge_30_proximo"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:fontFamily="@string/fonteFamily"
        android:textSize="@dimen/tam_font_normal"
        android:textColor="@color/cor_fonte"/>
</RelativeLayout>
